    <template id="schulmanager.schuelerview" template="" lang="" group="0" version="0.0.001">
        <template id="schulmanager.schuelerview.navi"/>
        <hidden id="token"/>
        <grid width="100%">
            <columns>
                <column/>
                <column/>
            </columns>
            <rows>
                <row class="dialogHeader" height="28">
                    <description id="header_klasse" class="sm_label"/>
                    <description id="header_schuelername" class="sm_label"/>
                </row>
                <row>
                    <tabbox id="tabs" span="all" >
                        <tabs>
                            <tab id="contact" label="Kontakt"/>
                            <tab id="schullaufbahn" label="Schullaufbahn"/>
                            <tab id="notenbild" label="Notenbild"/>
                            <!--<tab id="notenbild" label="Notenbild2"/>-->
                            <!--<tab id="zzeugnis" label="Zwischenzeugnis"/>
                            <tab id="jzeugnis" label="Jahreszeugnis"/>
                            <tab id="notenbericht" label="Notenbericht"/>-->
                        </tabs>
                        <tabpanels>
                            <template id="schulmanager.schuelerview.contact"/>
                            <template id="schulmanager.schuelerview.schullaufbahn"/>
                            <template id="schulmanager.schuelerview.notenbild"/>
                            <!--<template id="schulmanager.schuelerview.notenbild2"/>-->
                            <!--<template id="schulmanager.schuelerview.zwischenzeugnis"/>
                            <template id="schulmanager.schuelerview.jahreszeugnis"/>
                            <template id="schulmanager.schuelerview.notenbericht"/>-->
                        </tabpanels>
                    </tabbox>
                </row>
            </rows>
        </grid>
    </template>

</overlay>